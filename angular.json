{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ecom-angu": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ecom-angu",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/normalize.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/material-design-iconic-font.min.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/animate.css",
              "src/assets/css/TimeCircles.css",
              "src/assets/css/owl.carousel.css",
              "src/assets/css/slicknav.min.css",
              "src/assets/lib/css/nivo-slider.css",
              "src/assets/lib/css/preview.css",
              "src/assets/css/jquery-ui.css",
              "src/assets/style.css",
              "src/assets/css/responsive.css",
              "src/assets/css/custom.css"
            ],
            "scripts": [
            ],
            "allowedCommonJsDependencies": [
              "src/assets/js/vendor/modernizr-2.8.3.min.js",
              "src/assets/js/vendor/jquery-1.12.4.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/jquery-price-slider.js",
              "src/assets/js/TimeCircles.js",
              "src/assets/js/jquery.easing.js",
              "src/assets/js/jquery.slicknav.min.js",
              "src/assets/js/owl.carousel.min.js",
              "src/assets/lib/js/jquery.nivo.slider.js",
              "src/assets/lib/home.js",
              "src/assets/js/plugins.js",
              "src/assets/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ecom-angu:build:production"
            },
            "development": {
              "browserTarget": "ecom-angu:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ecom-angu:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
